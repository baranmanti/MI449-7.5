{
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "dependencies": {
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i styles/styles.css -o _site/styles/styles.css --watch --postcss",
    "build:11ty": "eleventy",
    "build:css": "tailwindcss -i styles/styles.css -o _site/styles/styles.css --postcss"
  }
}
